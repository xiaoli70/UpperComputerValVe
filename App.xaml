<Application x:Class="EquipmentSignalData.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EquipmentSignalData"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
            
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Light.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesign2.Defaults.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignColors;component/Themes/Recommended/Primary/MaterialDesignColor.BlueGrey.xaml"/>
                
            </ResourceDictionary.MergedDictionaries>
            <FontFamily x:Key="iconfont">pack://application:,,,/EquipmentSignalData;component/Assets/Fonts/#iconfont</FontFamily>
            <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="#77787b"/>
            
            
        </ResourceDictionary>
        
            
        
    </Application.Resources>
</Application>
